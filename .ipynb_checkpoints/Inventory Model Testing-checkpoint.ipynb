    "# print()\n",
    "\n",
    "# for i in range(1,6):\n",
    "#     def constant_model(env):\n",
    "#         def policy(state):\n",
    "#             return i\n",
    "#         return policy\n",
    "#     print ('discrete state space under constant %s order policy:' %i)\n",
    "#     evaluate(constant_model, env, numiter)\n",
    "#     print()\n",
    "\n",
    "L = 100\n",
    "res_arr = []\n",
    "res_std_arr = []\n",
    "\n",
    "for p in [0.25,1,4,9,39,99]:\n",
    "    ContCONFIG = {'h': 1, 'p': p, 'L': L, 'lambda': 1}\n",
    "    cont_env = gym.make('inventory_cont_config_validation_model-v0', \n",
    "                    config=ContCONFIG)\n",
    "    sub_res = []\n",
    "    sub_res_std = []\n",
    "    for i in range(0,22):\n",
    "        def constant_model(env):\n",
    "            def policy(state):\n",
    "                return np.array([i/20])\n",
    "#                 return np.array([0])\n",
    "    #             return i/100\n",
    "            return policy\n",
    "        print ('continuous state space under constant %s order policy:' %(i/20))\n",
    "        avg, std = evaluate(constant_model, cont_env, numiter)\n",
    "        sub_res.append(avg)\n",
    "        sub_res_std.append(std)\n",
    "        print()\n",
    "    res_arr.append(-max(sub_res))\n",
    "    res_std_arr.append(sub_res_std[np.argmax(sub_res)])"
   ]
